{
  "name": "undertext/arouter",
  "description": "Annotation based router",
  "keywords": [
    "router",
    "psr-7"
  ],
  "homepage": "https://github.com/undertext/arouter",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "doctrine/annotations": "^1.6",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ARouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ARouter\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "dev-master"
  }
}
